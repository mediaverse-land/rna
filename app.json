{
  "expo": {
    "name": "Mediaverse",
    "slug": "mediaverse",
    "version": "0.0.09",
    "orientation": "portrait",
    "icon": "./assets/flat_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/iphone-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#090951"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.emahdi1297.mediaverse"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/flat_icon.png",
        "backgroundColor": "#030340"
      },
      "package": "com.emahdi1297.mediaverse"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "notification": {
      "icon": "./assets/logo-mediaverse.png",
      "color": "#597AFF"
    },
    "plugins": [
      "@react-native-google-signin/google-signin",
      "@react-native-firebase/app",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/logo-mediaverse.png",
          "color": "#597AFF",
          "sounds": []
        }
      ],
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "2ecc9ab0-6195-4d80-97b7-5e15f2a89bbe"
      }
    },
    "owner": "emahdi1297"
  }
}
